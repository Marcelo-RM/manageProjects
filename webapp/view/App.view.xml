<mvc:View controllerName="manageprojects.manageprojects.controller.App" xmlns:mvc="sap.ui.core.mvc" displayBlock="true" xmlns="sap.m">
	<Shell id="shell">
		<App id="app">
			<pages>
				<Page id="page" title="{i18n>titleMaster}">
					<content>
						<List id="projectList"
							items="{
								path: '/projetos'
							}">
							<ObjectListItem  title="{nome}" number="{id}"  type="Navigation"  
								press=".selectProject" >
							</ObjectListItem>
						</List>
					</content>
				</Page>
			</pages>
		</App>
	</Shell>
</mvc:View>